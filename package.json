{
  "name": "bhousereminderapp",
  "version": "1.0.0",
  "description": "Reminding of your contribution in your bhouse.",
  "scripts": {
    "install": "run-p install:api install:web",
    "install:api": "cd root/api && npm install",
    "install:web": "cd root/web && npm install",
    "start": "run-p start:api start:web",
    "start:api": "npm run start --prefix root/api",
    "start:web": "npm run start --prefix root/web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khertg/BhouseReminderApp.git"
  },
  "keywords": [
    "reminder",
    "bhouse"
  ],
  "author": "Khert Geverola",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khertg/BhouseReminderApp/issues"
  },
  "homepage": "https://github.com/khertg/BhouseReminderApp#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
